	partition DateTable2 = calculated
		mode: import
		source = ```
				
				ADDCOLUMNS(
				    CALENDAR(DATE(2015, 1, 1), DATE(2030, 12, 31)),  -- Set start and end dates
				    "Year", YEAR([Date]),
				    "Month", FORMAT([Date], "MMMM"),  -- Full month name (e.g., January)
				    "Month Number", MONTH([Date]),
				    "Quarter", "Q" & FORMAT([Date], "Q"),
				    "Weekday", FORMAT([Date], "dddd"),  -- Full weekday name (e.g., Monday)
				    "Weekday Number", WEEKDAY([Date], 2),  -- 2 = Monday as start of week
				    "Year-Month", FORMAT([Date], "YYYY-MM") -- Useful for sorting
